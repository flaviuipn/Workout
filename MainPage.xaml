<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="RentACarM.MainPage">

    <VerticalStackLayout Spacing="10" Margin="10">
        <HorizontalStackLayout Spacing="10">
            <Image Source="gym.png"
       SemanticProperties.Description="Gym Logo"
       HeightRequest="300" />

            <Label FontSize="22" FontAttributes="Bold" Text="! Workout GYM !" VerticalOptions="End" />
            <Label FontSize="22" Text="v1.0" VerticalOptions="End" />
        </HorizontalStackLayout>
        <Label Text="Welcome to your favorite gym app." />
       
    </VerticalStackLayout>

</ContentPage>
